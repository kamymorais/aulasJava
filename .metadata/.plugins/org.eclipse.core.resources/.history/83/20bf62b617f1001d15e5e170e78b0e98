package principal;

public class Program {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int numero1 = 25;
		int numero2 = 10;
		int numero3 = 2;
		
		if (numero1> numero2) {
		System.out.println("Numero 1 é o maior");
		
		if (numero1> numero3) {
			System.out.println("Numero 1 é o maior");	
			
			if (numero2> numero1) {
				System.out.println("Numero 2 é o maior");
			
				if (numero2> numero3) {
					System.out.println("Numero 2 é o maior");
			
					if (numero3> numero1) {
						System.out.println("Numero 3 é o maior");
						
						if (numero3> numero2) {
							System.out.println("Numero 3 é o maior");
			
						}}}}}}
	}

}
